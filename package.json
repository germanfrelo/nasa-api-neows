		"@parcel/transformer-css-experimental": "2.2.1",
		"eslint": "8.7.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-react": "7.28.0",
		"parcel": "2.2.1",
		"prettier": "2.5.1"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
